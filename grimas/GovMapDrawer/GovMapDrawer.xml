<GrimaForm>
    <Title>Gov Map Drawer Change</Title>
	<Field
	name="whichnote"
	label="Alma Map Location"
	type="select"
	required="yes"
	placeholder="Alma Code"
	>
	<option value="govoffmap">govoffmap</option>
	<option value="govmap">govmap</option>
	</Field>
	<Field
		name="olddrawer"
		label="Old Drawer Number"
		required="yes"
		placeholder="Old Drawer Number"
		/>
	<Field
		name="newdrawer"
		label="New Drawer Number"
		required="yes"
		placeholder="New Drawer Number"
	/>
    <Field
        name="holding_id"
        label="Bibs with holdings without items"
        autocomplete="off"
        required="yes"
        placeholder="Holding Id without items, one per line."
		rows="5"
    />
</GrimaForm>
