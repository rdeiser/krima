<GrimaForm>
    <Title>WDGovDoc New Item Record</Title>
	<Field
	name="whichnote"
	label="Stat 3 Note:"
	type="select"
	required="yes"
	placeholder="Stat 3 Note"
	>
	<option value="NOT KU FDLP REQUEST">NOT KU FDLP REQUEST</option>
	<option value="KU FDLP REQUEST">KU FDLP REQUEST</option>
	<option value="GOV UNBOXING review">GOV UNBOXING review</option>
	</Field>
	<Field
		name="holding_id"
		label="Holding ID"
		required="yes"
	/>
	<Field
        name="chronology_i"
		label="Chron Year"
        required="no"
        visible="yes"
    />
	<Field
        name="description"
        required="no"
        visible="no"
    />
	<Field
        name="barcode"
        label="Barcode"
        autocomplete="off"
        required="no"
        placeholder="Barcode found on piece"
    />
</GrimaForm>
