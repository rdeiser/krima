<GrimaForm>
    <Title>Annex Transfer--Update Holdings</Title>
	<Field
	name="location"
	label="Annex Location Code"
	type="select"
	required="yes"
	>
	<option value="annex">annex</option>
	<option value="annexltd">annexltd</option>
	<option value="KS-Extension">KS-Extension</option>
	<option value="govstorks">govstorks</option>
	</Field>
    <Field
        label="Barcode of unboxed item"
        name="unboxed_barcode"
        required="yes"
        placeholder="Scan barcode..."
    />
	<Field
		name="inventorydate"
		date-type="all"
		type="date"
		required="no"
		visible="no"
		/>
</GrimaForm>
