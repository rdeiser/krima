<GrimaForm onsubmit="return false">
    <Title>New Item Record Creation</Title>
	<!--<Field
	name="mms"
	label="MMS ID"
	required="yes"
	/>--->
	<Field
	name="holding_id"
	label="Holdings Record without an Item Record"
	autocomplete="off"
	required="yes"
	placeholder="Holding Id Number without an Item Record"
    />
	<!--<Field
	name="copyid"
	label="Copy ID"
	required="no"
	/>-->
	<Field
	name="itempolicy"
	label="Item Policy"
	required="no"
	type="select"
	>
	<option value="book/ser">book/ser</option>
	<option value="7 day">7 day</option>
	<option value="no loan">no loan</option>
	</Field>
	<Field
	name="pieces"
	label="Pieces"
	required="no"
	/>
	<Field
	name="pubnote"
	label="Public Note"
	required="no"
	/>
	<Field
	name="fulnote"
	label="Fulfillment Note"
	required="no"
	placeholder="Fulfillment Note"
	/>
	<Field
	name="statnote1"
	label="Statistics Note 1"
	type="select"
	required="no"
	>
	<option></option>
	<option value="MISSING 2020">MISSING 2020</option>
	<option value="LOST 2020">LOST 2020</option>
	<option value="WITHDRAWN">WITHDRAWN</option>
	<option value="WITHDRAWN DUE TO 2018 FIRE">WITHDRAWN DUE TO 2018 FIRE</option>
	</Field>
	<Field
	name="statnote2"
	label="Statistics Note 2"
	type="select"
	required="no"
	>
	<option value="FIRE 2018 OZONE GAMMA">FIRE 2018 OZONE GAMMA</option>
	<!--<option></option>-->
	<option  value="FIRE 2018 OZONE">FIRE 2018 OZONE</option>
	<!--<option value="FIRE 2018 OZONE GAMMA">FIRE 2018 OZONE GAMMA</option>-->
	<option value="FIRE 2018 OZONE GAMMA REBIND">FIRE 2018 OZONE GAMMA REBIND</option>
	<option value="FIRE 2018 SPECIAL COLLECTIONS">FIRE 2018 SPECIAL COLLECTIONS</option>
	<option value="LACKS ozone">LACKS ozone</option>
	<option></option>
	</Field>
	<Field
	name="statnote3"
	label="Statistics Note 3"
	type="select"
	required="no"
	placeholder="Unboxing Note"
	>
	<option></option>
	<option value="ANNEX ingest">ANNEX ingest</option>
	<option value="HALE return">HALE return</option>
	<option value="To be WITHDRAWN">To be WITHDRAWN</option>
	<option value="AHD HALE return">AHD HALE return</option>
	<option value="AHD ANNEX ingest">AHD ANNEX ingest</option>
	<option value="AHD To be WITHDRAWN">AHD To be WITHDRAWN</option>
	</Field>
	<Field
        label="Barcode"
        name="barcode"
        required="no"
        placeholder="Scan barcode..."
    />
</GrimaForm>
